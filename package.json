{
	"name": "cssgenerator",
	"displayName": "cssGenerator",
	"description": "",
	"publisher": "Kazuki",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Formatters",
		"Snippets",
		"Other"
	],
	"keywords": [
		"css"
	],
	"activationEvents": [
		"onCommand:CssGenerator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "CssGenerator",
				"title": "Generate CSS"
			}
		],
		"configuration":{
			"title": "CssGenerator configuration",
			"properties": {
				"CssGenerator.minWidth": {
						"type": "number",
						"default": 768,
						"description": "minWidth"
				},
				"CssGenerator.maxWidth": {
						"type": "number",
						"default": 767,
						"description": "maxWidth"
				},
				"CssGenerator.resetCss": {
						"type": "boolean",
						"default": false,
						"description": "resetCss"
				},
				"CssGenerator.ignorantClass": {
						"type": "string",
						"default": "",
						"description": "Please enter the class name you do not want to be reflected in the css file, separated by commas."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
